{
    "name": "rd-component",
    "version": "0.1.143",
    "type": "module",
    "description": "Reddwarf public component lib",
    "files": [
        "dist"
    ],
    "main": "./dist/rd-component.es.js",
    "module": "./dist/rd-component.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/rd-component.es.js"
        },
        "./dist/style.css": {
            "import": "./dist/style.css",
            "require": "./dist/style.css"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "export NODE_ENV=production && tsc && vite build --mode production",
        "watch": "vite build --watch",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "jest",
        "storybook": "storybook dev --host 0.0.0.0 --port 9001 -c .storybook"
    },
    "jest": {
        "transform": {
            "^.+\\.ts?$": "babel-jest",
            "^.+\\.jsx?$": "babel-jest",
            "^.+\\.tsx$": "ts-jest"
        }
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@reduxjs/toolkit": "^1.9.5",
        "@storybook/cli": "^7.0.21",
        "@storybook/react": "^7.0.21",
        "@storybook/react-vite": "^7.0.21",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/event-source-polyfill": "^1.0.1",
        "@types/jest": "^29.5.2",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.3.1",
        "@types/react": "^18.2.0",
        "@types/redux-logger": "^3.0.9",
        "axios": "^1.4.0",
        "event-source-polyfill": "^1.0.31",
        "rdjs-wheel": "0.0.23",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.15.0",
        "react-redux": "^8.1.0",
        "redux": "^4.2.1",
        "redux-logger": "^3.0.6",
        "uuid": "^9.0.0",
        "vite": "^4.3.9",
        "@fingerprintjs/fingerprintjs": "^4.0.0"
    },
    "peerDependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.15.0",
        "react-redux": "^8.0.5",
        "redux": "^4.2.1",
        "uuid": "^9.0.0",
        "@fingerprintjs/fingerprintjs": "^4.0.0"
    },
    "dependencies": {
        "@babel/core": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@types/react": "^18.2.12",
        "@types/redux-logger": "^3.0.9",
        "@types/uuid": "^9.0.2",
        "@vitejs/plugin-react": "^4.0.0",
        "babel-jest": "^29.5.0",
        "jest": "^29.5.0",
        "query-string": "^8.1.0",
        "storybook": "^7.0.21",
        "ts-jest": "^29.1.0",
        "vite-plugin-dts": "^2.3.0",
        "react-toastify": "^9.1.3"
    }
