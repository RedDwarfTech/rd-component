{
  "compilerOptions": {
    "typeRoots": [
      "./typings"
    ],
    "types": [
      "@types/jest"
    ],
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*",
      ],
      "rdjs-wheel": ["node_modules/rdjs-wheel"]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "declaration": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "typings/typings.d.ts"
  ],
  "test": {
    "compilerOptions": {
      "jsx": "react-jsx"
    }
  }
}